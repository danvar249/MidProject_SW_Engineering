<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EktRegionalManagerAcceptNewCustomerController">
   <children>
      <Pane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: linear-gradient(from -200px 0px to 0px 1400px, #a837b4, transparent);">
         <children>
            <Pane prefHeight="120.0" prefWidth="800.0">
               <children>
                  <Text fill="WHITE" layoutX="198.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ACCEPT NEW CUSTOMERS">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="30.0" />
                     </font>
                  </Text>
                  <ImageView fitHeight="200.0" fitWidth="200.0" layoutY="-33.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Images/logo%20final%20-%20yellow.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <TableView fx:id="tableUsers" layoutX="50.0" layoutY="133.0" prefHeight="595.0" prefWidth="700.0" style="-fx-background-color: linear-gradient(from -200px 0px to 0px 1400px, #a837b4, transparent);">
              <columns>
                <TableColumn fx:id="columnName" prefWidth="130.0" text="Name" />
                <TableColumn fx:id="columnUserType" minWidth="0.0" prefWidth="130.0" text="User Type" />
                  <TableColumn fx:id="columnID" prefWidth="110.0" text="ID" />
                  <TableColumn fx:id="columnPhoneNumber" prefWidth="180.0" text="Phone Number" />
              </columns>
            </TableView>
            <Button fx:id="btnBack" layoutX="14.0" layoutY="744.0" mnemonicParsing="false" onAction="#getBtnBack" style="-fx-background-radius: 200; -fx-border-radius: 200;" stylesheets="@buttonCSS.css" text="BACK" textFill="WHITE">
               <font>
                  <Font name="Arial Rounded MT Bold" size="20.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
